@{
    ViewBag.Title = "Index";
}

<h2>Progress of: @ViewBag.StudentName</h2>

<table class="table">
    <tr>
        <th>
            Date
        </th>
        <th>
            Completed
        </th>
        <th></th>
    </tr>

@foreach (var date in ViewBag.dateOfProgresses) {
    <tr>
        <td>
            @date
        </td>
        <td>
           No
        </td>
        <td>
        </td>
    </tr>
}

</table>
