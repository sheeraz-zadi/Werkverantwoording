@model  Werkverantwoording.Models.Assignment

@{
    ViewBag.Title = "Index";
}

<h2>Assignments done on: @ViewBag.DayDateTime</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Description)
        </th>
        <th></th>
    </tr>

@foreach (var assignment in ViewBag.AssignmentsDesc)
{
    <tr>
        <td>
            @assignment.Description
        </td>
    </tr>
}

</table>
